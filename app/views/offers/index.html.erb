<div class="container">
  <div class="mt-3">
    <h1>Avaliable consultations</h1>
    <% @offers.each do |offer| %>
      <% if offer.available %>
        <h3>Consultation begins <%= link_to offer.start_date, offer_path(offer) %></h3>
      <% end %>
    <% end %>
  </div>
  <%= link_to "Back", consultations_path %>
</div>
